{
    "env": { "node": true, "es6": true, "mocha": true },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "parserOptions": { "ecmaVersion": 2020 },
    "rules": {
        "no-unused-vars": ["error", { "varsIgnorePattern": "[iI]gnored", "argsIgnorePattern": "^_" }],
        "no-console": 0,
        "node/no-unpublished-require": "off"
    }
}
